{
  "expo": {
    "name": "{{name}}",
    "description": "{{description}}",
    "slug": "{{slug}}",
    "privacy": "unlisted",
    "sdkVersion": "{{expoSDKVersion}}",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "{{version}}",
    "orientation": "portrait",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#fffff"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/icon.png",
      "bundleIdentifier": "{{bundleId}}",
      "buildNumber": "1",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/icon.png",
      "package": "{{bundleId}}",
      "versionCode": 1
    }
  }
}